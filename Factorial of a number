#include <stdio.h>
#include <stdlib.h>

int fact (int n);

int fact(int n)
{
    int i, f = 1;
    for(i=1;i<=n;i++)
    {
        f = f*i;
    }
    return f;
}

int main()
{
    int n, result;
    printf("Enter a Number");
    scanf("%d",&n);
    result = fact(n);
    printf("%d",result);
    return 0;
}
